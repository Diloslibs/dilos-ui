{
  "name": "@dilos/preline",
  "private": false,
  "version": "0.0.3",
  "author": {
    "name": "Dilos Core Team"
  },
  "description": "A collection of libraries for Dilos",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "plugin.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Diloslibs/dilos-ui.git"
  },
  "scripts": {
    "start": "concurrently pnpm:start:*",
    "start:webpack": "webpack --mode=development --watch",
    "start:tailwind": "npx tailwindcss -i ./src/styles/index.css -o ./dist/index.css --watch",
    "build-html": "concurrently pnpm:build-html:*",
    "build-html:css": "tailwindcss -i ./src/styles/index.css -o ./dist/index.css --minify",
    "build-html:ts": "webpack --mode=production --config=tools/webpack/html/webpack.config.js",
    "build-npm": "webpack --config=tools/webpack/npm/webpack.config.js",
    "generate-dts": "dts-bundle-generator --config dts-config.js"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "dts-bundle-generator": "^9.5.1",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss-loader": "^8.1.1",
    "rm": "^0.1.8",
    "source-map-loader": "^5.0.0",
    "tailwindcss": "^3.4.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.2",
    "vite-plugin-dts": "^3.7.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8"
  }
}
