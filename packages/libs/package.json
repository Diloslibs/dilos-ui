{
  "name": "@dilos/libs",
  "private": false,
  "version": "0.0.4",
  "author": {
    "name": "Dilos Core Team"
  },
  "description": "A collection of libraries for Dilos",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Diloslibs/dilos-ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "concurrently pnpm:start:*",
    "start:webpack": "webpack --mode=development --watch",
    "start:tailwind": "npx tailwindcss -i ./src/styles/index.css -o ./dist/index.css --watch",
    "build": "concurrently pnpm:build:*",
    "build:css": "tailwindcss -i ./src/styles/index.css -o ./dist/index.css --minify",
    "build:ts": "webpack --mode=production"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "concurrently": "^8.2.2",
    "rm": "^0.1.8",
    "source-map-loader": "^5.0.0",
    "tailwindcss": "^3.4.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.2.2",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8"
  }
}