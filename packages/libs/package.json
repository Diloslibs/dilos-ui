{
  "name": "libs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently pnpm:start:*",
    "start:rm": "rm -rf ./dist",
    "start:css": "tailwindcss -i ./src/styles/index.css -o ./dist/index.css --watch",
    "start:ts": "tsc -w",
    "build": "concurrently pnpm:build:*",
    "build:css": "tailwindcss -i ./src/styles/index.css -o ./dist/index.css --minify",
    "build:ts": "vite build"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "concurrently": "^8.2.2",
    "rm": "^0.1.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.2",
    "vite-plugin-dts": "^3.7.3"
  }
}